{
  "name": "ts-test-0427",
  "version": "1.0.0",
  "description": "A ts test repo",
  "main": "dist/tsTest.js",
  "types": "dist/tsTest.d.js",
  "module": "src/tsTest.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint-fix": "eslint --fix 'src/**/*.{js,ts}'",
    "prettier": "prettier --write --config .prettierrc.js 'src/**/*.{js,ts}'",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests",
    "test-c": "cross-env NODE_ENV=test jest --coverage"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangfupeng1988/ts-test-0427.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wangfupeng1988/ts-test-0427/issues"
  },
  "homepage": "https://github.com/wangfupeng1988/ts-test-0427#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "autoprefixer": "^9.7.6",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "lint-staged": "^10.2.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "jest": "^26.0.1",
    "lodash": "^4.17.15",
    "strip-ansi": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run prettier",
      "npm run lint-fix",
      "git add ."
    ]
  }
}
